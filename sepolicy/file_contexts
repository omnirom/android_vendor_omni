###########################
# OmniROM common sepolicy
#

# performance-related sysfs files
/sys/kernel/mm/ksm(/.*)?    u:object_r:sysfs_ksm:s0
/sys/block/mmcblk[012]/queue(/.*)     u:object_r:sysfs_ioscheduler:s0
(/.*)scheduler      u:object_r:sysfs_ioscheduler:s0
(/.*)read_ahead_kb      u:object_r:sysfs_ioscheduler:s0
/sys/block/zram(/.*)?       u:object_r:sysfs_zram:s0
